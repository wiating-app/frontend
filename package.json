  "scripts": {
    "prepare": "eval ./prepare.js",
    "start": "yarn prepare && webpack serve --config webpack.dev.js",
    "start:netlify": "yarn prepare && concurrently --kill-others 'yarn start' 'netlify dev'",
    "build": "yarn prepare && webpack --config webpack.prod.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime",
      [
        "babel-plugin-import",
        {
          "libraryName": "@material-ui/core",
          "libraryDirectory": "esm",
          "camel2DashComponentName": false
        },
        "core"
      ],
      [
        "babel-plugin-import",
        {
          "libraryName": "@material-ui/icons",
          "libraryDirectory": "esm",
          "camel2DashComponentName": false
        },
        "icons"
      ]
    ],
    "targets": {
      "browsers": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.6.2",
